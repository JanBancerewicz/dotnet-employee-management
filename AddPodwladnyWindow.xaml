<Window x:Class="dotnet.AddPodwladnyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dotnet"
        mc:Ignorable="d"
        Title="AddPodwladnyWindow" Height="282" Width="254">
    <StackPanel>
        <TextBlock x:Name="ImieLabel" TextWrapping="Wrap" Text="Imie" HorizontalAlignment="Center"/>
        <TextBox x:Name="ImieInput" TextWrapping="Wrap" Width="120" HorizontalAlignment="Center"/>
        <TextBlock x:Name="NazwiskoLabel" TextWrapping="Wrap" Text="Nazwisko" HorizontalAlignment="Center"/>
        <TextBox x:Name="NazwiskoInput" TextWrapping="Wrap" Width="120"/>
        <TextBlock x:Name="StanowiskoLabel" TextWrapping="Wrap" Text="Stanowisko" HorizontalAlignment="Center"/>
        <TextBox x:Name="StanowiskoInput" TextWrapping="Wrap" Width="120"/>
        <TextBlock x:Name="PensjaLabel" TextWrapping="Wrap" Text="Pensja" HorizontalAlignment="Center"/>
        <TextBox x:Name="PensjaInput" TextWrapping="Wrap" Width="120"/>
        <TextBlock x:Name="StazLabel" TextWrapping="Wrap" Text="Staz" HorizontalAlignment="Center"/>
        <TextBox x:Name="StazInput" TextWrapping="Wrap" Width="120"/>
        <Button Content="Generuj" Click="Generuj_Click" Margin="15,15,15,15"/>
        <Button Content="Dodaj" Click="Dodaj_Click" Margin="15,0,15,15"/>
    </StackPanel>
</Window>
